    "pubsub-js": "^1.9.4",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-git-info": "^2.0.1",
    "react-helmet-async": "^1.2.2",
    "react-i18next": "^11.15.3",
    "react-markdown": "^8.0.0",
    "react-text-mask": "^5.4.3",
