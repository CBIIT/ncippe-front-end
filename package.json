  "engines": {
    "node": ">=10.16.0",
    "npm": "6.9.x"
  },
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.7.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.35",
    "@reach/router": "^1.2.1",
    "copyfiles": "^2.1.1",
    "core-js": "^3.4.8",
    "env-cmd": "^10.0.1",
    "fsevents": "^2.1.2",
    "i18next": "^19.0.1",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "json-server": "^0.15.1",
    "lunr": "^2.3.8",
    "moment": "^2.24.0",
    "ncippe-content": "github:CBIIT/ncippe-content",
    "oidc-client": "^1.9.1",
    "query-string": "^6.9.0",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.5",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.2.5",
    "react-markdown": "^4.2.2",
    "react-number-format": "^4.3.1",
    "react-scripts": "^3.3.0",
    "react-text-mask": "^5.4.3",
    "react-tracking": "^7.2.1",
    "react-transition-group": "^4.3.0",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "scripts": {
    "test-server": "npm run copyfiles & react-scripts start & node server.js",
    "dev-server": "./node_modules/.bin/env-cmd -e local react-scripts start & npm run mock",
    "start": "npm run copyfiles & ./node_modules/.bin/env-cmd -e local react-scripts start",
    "start_deploy": "npm run copyfiles & node server.js",
    "build": "npm run copyfiles & react-scripts build",
    "test": "npm run copyfiles & react-scripts test",
    "eject": "react-scripts eject",
    "mock": "json-server --watch src/data/mockData.json --port 5000 --routes ./routes.json --middlewares ./server_getUser.js ./server_getProviders.js ./server_singular.js",
    "copyfiles": "copyfiles -u 2 ./node_modules/ncippe-content/locales/**/*.json ./public"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "import/no-webpack-loader-syntax": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ]
  },
  "proxy": "http://localhost:5000"
}
